UNITS LENGTH=A TIME=ps ENERGY=kcal/mol

MAZE_LOSS ...
  LABEL=l

  PARAMS=1,1,1
... MAZE_LOSS

MAZE_RANDOM_WALK ...
  LABEL=opt

  LOSS=l
  OPTIMIZER_STRIDE=10
  
  LIGAND=2635-2646
  PROTEIN=1-2634

  NLIST
  NL_CUTOFF=7
  NL_STRIDE=100
... MAZE_RANDOM_WALK

p: POSITION ATOM=2635 NOPBC

MAZE_OPTIMIZER_BIAS ...
  LABEL=b  

  ARG=p.x,p.y,p.z

  BIASING_RATE=0.02
  ALPHA=7.2
  OPTIMIZER=opt
... MAZE_OPTIMIZER_BIAS

PRINT ...
  ARG=opt.sr,p.*
  FILE=colvar 
  FMT=%15.8f 
... PRINT
  
