NOE ...
GROUPA1=1,3 GROUPB1=2,2 NOEDIST1=0.5
GROUPA2=5 GROUPB2=7 NOEDIST2=0.4
LABEL=prova
... NOE
NOE ...
GROUPA1=1,3 GROUPB1=2,2 NOEDIST1=0.5
GROUPA2=5 GROUPB2=7 NOEDIST2=0.4
LABEL=prova2
... NOE

test: DISTANCE ATOMS=5,7

enoe: ENSEMBLE ARG=(prova\.noe-.*)
enoe2: ENSEMBLE ARG=(prova2\.noe-.*) POWER=-0.16667

inver1: COMBINE ARG=enoe.prova.noe-0 POWERS=-0.16667 PERIODIC=NO
inver2: COMBINE ARG=enoe.prova.noe-1 POWERS=-0.16667 PERIODIC=NO

st1: STATS ARG=inver1,inver2 PARARG=(prova\.exp-.*) SQDEVSUM
st2: STATS ARG=enoe2.* PARARG=(prova2\.exp-.*) SQDEVSUM

RESTRAINT ARG=st1.*,st2.* AT=0,0 SLOPE=1,1 KAPPA=0,0

DUMPFORCES ARG=(prova\.noe-.*),(prova2\.noe-.*) STRIDE=1 FILE=forces


PRINT ARG=inver1,inver2,test,(prova\.exp-.*),st1.*,st2.* FILE=COLVAR STRIDE=1

d1: DISTANCE ATOMS=1,10
d2: DISTANCE ATOMS=2,20
cc: COMBINE ARG=d1,d2 PERIODIC=NO

NOE ...
GROUPA1=1,3 GROUPB1=20,20 NOEDIST1=10
GROUPA2=3 GROUPB2=17 NOEDIST2=50
DOSCORE
SIGMA_MEAN0=1
SIGMA0=0 DSIGMA=0
TEMP=300
ARG=cc
REWEIGHT
LABEL=nost
... NOE

b: BIASVALUE ARG=nost.score STRIDE=2

PRINT ARG=nost.weight,b.bias STRIDE=2 FILE=METAI
